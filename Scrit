pipeline{
  agent any
  stages{
    stage('check validate'){
      steps{
               echo 'iam validating the code'
           } 
    stage('check compilation'){
      steps{
               echo 'iam compilng the code'
           } 
    stage('check install'){
      steps{
               echo 'iam installing the code'
           } 
    stage('check deploy'){
      steps{
               echo 'iam deploying the code'
           } 
    }
}
}
